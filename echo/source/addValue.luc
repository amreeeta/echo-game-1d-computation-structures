module addValue (
    input sequenceCount[16],  // clock
    output addvalue[16]  // reset
  ) {

  always {
  
    case (sequenceCount) {
                b0000000000000001: addvalue = b0000000000000001; 
                b0000000000000010: addvalue = b0000000000000010; 
                b0000000000000011: addvalue = b0000000000000100; 
                b0000000000000100: addvalue = b0000000000001000;  
                b0000000000000101: addvalue = b0000000000010000; 
                b0000000000000110: addvalue = b0000000000100000; 
                b0000000000000111: addvalue = b0000000001000000; 
                b0000000000001000: addvalue = b0000000010000000; 
                b0000000000001001: addvalue = b0000000100000000; 
                b0000000000001010: addvalue = b0000001000000000; 
                b0000000000001011: addvalue = b0000010000000000; 
                b0000000000001100: addvalue = b0000100000000000; 
                b0000000000001101: addvalue = b0001000000000000;  
                b0000000000001110: addvalue = b0010000000000000; 
                b0000000000001111: addvalue = b0100000000000000; 
                b0000000000010000: addvalue = b1000000000000000; 
              default: addvalue = b0000000000000000;
              }
  }
}
